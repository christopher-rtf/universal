{
    "com.apple.macos.language": {
        "name": "macOS Display Language",
        "contexts": {
            "OS": [
                {
                    "id": "darwin"
                }
            ]
        },
        "settingsHandlers": {
            "configure": {
                "type": "gpii.macos.updateLanguage",
                "liveness": "live",
                "capabilities": [
                    "http://registry\\.gpii\\.net/common/language"
                ],
                "capabilitiesTransformations": {
                    "currentLanguage": {
                        "transform": {
                            "type": "fluid.transforms.value",
                            "inputPath": "http://registry\\.gpii\\.net/common/language"
                        }
                    }
                }
            },
        },
        "configure": [
            "settings.configure",
        ],
        "restore": [
            "settings.configure",
        ],
        "start": [ ],
        "stop": [ ],
        "isInstalled": [
            {
                "type": "gpii.deviceReporter.alwaysInstalled"
            }
        ]
    },
    "com.apple.macos.screenDPI": {
        "name": "MacOS DPI",
        "contexts": {
            "OS": [
                {
                    "id": "darwin"
                }
            ]
        },
        "settingsHandlers": {
            "configure": {
                "type": "gpii.macos.displaySettingsHandler",
                "liveness": "live",
                "supportedSettings": {
                    "screen-dpi": {
                        "schema": {
                            "title": "Screen DPI",
                            "description": "Screen DPI of the default display.",
                            "type": "number",
                            "minimum": 1,
                            "maximum": 5
                            // Taken from dpiWindows10.js, may not be appropriate
                        }
                    }
                },
                "capabilitiesTransformations": {
                    "screen-dpi": "http://registry\\.gpii\\.net/common/DPIScale"
                }
            }
        },
        "isInstalled": [
            {
                "type": "gpii.deviceReporter.alwaysInstalled"
            }
        ]
    },
    "com.apple.macos.screenResolution": {
        "name": "macOS Screen Resolution",
        "contexts": {
            "OS": [
                {
                    "id": "darwin"
                }
            ]
        },
        "settingsHandlers": {
            "configuration": {
                "type": "gpii.macos.displaySettingsHandler",
                "liveness": "liveRestart",
                "supportedSettings": {
                    "screen-resolution": {
                        "schema": {
                            "title": "Screen resolution",
                            "description": "Screen resolution of the default display.",
                            "type": "array"
                            // TODO: Discuss and fix this.
                            // TODO: Not sure about this type
                            // TODO: This must be filled in on the fly with available screen resolutions
                            //"items": {
                            //    "enum": []
                            //}
                        }
                    }
                }
            }
        },
        "isInstalled": [
            {
                "type": "gpii.deviceReporter.alwaysInstalled"
            }
        ]
    },
    "com.apple.macos.volumeControl": {
        "name": "macOS system volume",
        "contexts": {
            "OS": [
                {
                    "id": "darwin"
                }
            ]
        },
        "settingsHandlers": {
            "configure": {
                "type": "gpii.macos.nativeSettingsHandler",
                "liveness": "live",
                "options": {
                    "functionName": "Volume"
                },
                "supportedSettings": {
                    "Volume": {
                        "schema": {
                            "title": "System volume",
                            "description": "Changes the current system volume.",
                            "type": "number",
                            "default": 0.5,
                            "minimum": 0,
                            "maximum": 1,
                            "multipleOf": 0.01
                        }
                    }
                },
                "capabilitiesTransformations": {
                    "Volume": {
                        "transform": {
                            "type": "fluid.transforms.value",
                            "inputPath": "http://registry\\.gpii\\.net/common/volume",
                            "outputPath": "value"
                        }
                    }
                },
                "inverseCapabilitiesTransformations": {
                    "http://registry\\.gpii\\.net/common/volume": "Volume.value"
                }
            }
        },
        "start": [],
        "stop": [],
        "isInstalled": [
            {
                "type": "gpii.deviceReporter.alwaysInstalled"
            }
        ],
        "isRunning": []
    },
    "fakemag1": {
        "name": "Fake Magnifier 1",
        "contexts": {
            "OS": [
                {
                    "id": "darwin"
                }
            ]
        },
        "capabilities": [
            "http://registry\\.gpii\\.net/common/magnification/enabled"
        ],
        "settingsHandlers": {
            "configuration": {
                "type": "gpii.settingsHandlers.JSONSettingsHandler",
                "liveness": "live",
                "options": {
                    "filename": "/tmp/fakemag1.settings.json"
                },
                "capabilitiesTransformations": {
                    "magnification": "http://registry\\.gpii\\.net/common/magnification"
                }
            }
        },
        "configure": [
            "settings.configuration"
        ],
        "restore": [
            "settings.configuration"
        ],
        "start": [ ],
        "stop": [ ],
        "isInstalled": [
            {
                "type": "gpii.deviceReporter.alwaysInstalled"
            }
        ]
    },

